    "apollo-link": "^1.2.2",
    "rxjs": "^6.0.0-uncanny-rc.7"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "apollo-link": "1.2.8",
    "graphql": "0.13.2",
    "graphql-tag": "2.10.1",
    "jest": "23.6.0",
    "rimraf": "2.6.3",
    "rollup": "0.68.2",
    "rxjs": "6.4.0",
    "ts-jest": "22.4.6",
    "typescript": "3.3.1"
  }
}
